{
  "manifest_version": 2,

  "name": "COMP110 Office hours notifications",
  "description": "Click the 110 button on the queue pages and notifications will appear",
  "version": "1.2",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": {
    "74": "icon.png"
  },

  "permissions": ["activeTab"],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts":[
    {
      "matches": ["http://comp110.com/srm/?v=0.0.7#/staff/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
