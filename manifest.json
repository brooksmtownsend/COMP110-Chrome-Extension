{
  "manifest_version": 2,

  "name": "COMP110 Office hours notifications",
  "description": "This extension shows a popup whenever a student enters the queue.",
  "version": "1.10",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "COMP110 Notifcations Enabled"
  },

  "permissions": ["contextMenus"],

  "background": {
    "scripts": ["sample.js"]
  },

  "content_scripts": [
  {
    "matches": ["http://comp110.com/*"],
    "run_at": "document_end",
    "js": ["content.js"]
  }
]
}
