{
  "manifest_version": 2,

  "name": "COMP110 Office hours notifications",
  "description": "Click the 110 button on the queue pages and notifications will appear",
  "version": "1.10",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": ["activeTab"],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
  {
    "matches": ["http://comp110.com/*"],
    "run_at": "document_end",
    "js": ["content.js"]
  }
]
}
